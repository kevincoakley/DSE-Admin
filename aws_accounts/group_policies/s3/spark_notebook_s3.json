    {
      "Sid": "Stmt1427310301000",
      "Effect": "Allow",
      "Action": [
        "s3:*"
      ],
      "Resource": [
        "arn:aws:s3:::aws-logs-XXXXXXXXXXXX-us-east-1/*",
        "arn:aws:s3:::dse-team-20XX/*"
